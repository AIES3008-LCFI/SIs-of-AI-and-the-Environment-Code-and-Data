function isYoutubeRemembered()     {  return isCookieSetForDummy("youtube"); }
function isSoundcloudRemembered()     {  return isCookieSetForDummy("soundcloud"); }
function setCookieFor(coo, tra, fli, you, snd){
  var cc=getCookie();
  var jetzt = new Date();
  var Auszeit = new Date(jetzt.getTime() + 1000*3600*24*365);
  var coov="";
  var fliv="";
  var trav="";
  var youv="";
  var sndv="";
  if(coo == 1 || (coo == -1 && cc.indexOf("cookie")>-1))  {coov="cookie ";}
  if(fli == 1 || (fli == -1 && cc.indexOf("flickr")>-1))        { fliv="flickr ";}
  if(tra == 1 || (tra == -1 && cc.indexOf("tracker")>-1))   { trav="tracker ";}
  if(you == 1 || (you == -1 && cc.indexOf("youtube")>-1))   { trav="youtube ";}
  if(snd == 1 || (snd == -1 && cc.indexOf("soundcloud")>-1))   { trav="soundcloud ";}
  var zeit=Auszeit.toGMTString();
  if(coov+trav+fliv == "") zeit="Thu, 01 Jan 1970 00:00:00 UTC";
  newCookie = "bmvi="+coov+trav+fliv+"; expires=" + zeit + "; path=/";
  cl("new cookie: "+newCookie);
  document.cookie = newCookie;
}
function setCookieRemember(){  setCookieFor(1,-1,-1,-1,-1); }
